algoritmo "Exemlo vetores 01"

var
nomes: vetor [0..2] de caracter // vetor de 3 tipo posições, começando no 0 a posição 2

inicio
// vetor conjunto de espaços para aemazenar dados
nomes[0] := "Bernardo"
nomes[1] := "Genesis"
nomes[2] := "josé"

escreval ("Primeiro nome: ", nomes[0])
escreval ("Primeiro nome: ", nomes[1])
escreval ("Primeiro nome: ", nomes[2])


fimalgoritm
